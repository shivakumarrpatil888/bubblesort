#include<stdio.h>
int main()
{
         int a[20],n,i,j,,temp;
         printf("enter the element's\n");
         scanf(%d,&n);
         
         printf("enter %d integer's:"n);
         for(i=0p;i<n;i++)
         {
                 scanf("%d,&a[i]);
                 }
                  for(i=0;i<-1-j;i++)
                  {
                  for(j=0;,j<n-1-i;j++)
                  {
                  if(a[i]>a[i+1])
                           {
                           temp=a[i];
                           a[i]=a[i+1];
                           a[i+1]=temp;
                           }
                        }
                    }       
                   printf("the sorted array is..");
                   for(j=0;j<n;j++)
                   {
                              printf("%d\n",&a[i]);
                   }
                   
                   printf("\n"0;          
                   return 0;
                   }
      
